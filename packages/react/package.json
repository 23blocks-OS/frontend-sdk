{
  "name": "@23blocks/react",
  "version": "7.1.1",
  "description": "React bindings for 23blocks SDK - hooks and context providers",
  "license": "MIT",
  "author": "23blocks <hello@23blocks.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/23blocks-OS/frontend-sdk.git",
    "directory": "packages/react"
  },
  "homepage": "https://github.com/23blocks-OS/frontend-sdk/tree/main/packages/react",
  "bugs": {
    "url": "https://github.com/23blocks-OS/frontend-sdk/issues"
  },
  "keywords": [
    "23blocks",
    "react",
    "react-hooks",
    "react-context",
    "react-authentication",
    "react-sdk",
    "nextjs",
    "nextjs-auth",
    "react-api-client",
    "use-auth",
    "react-backend",
    "react-ecommerce",
    "react-crm",
    "baas-react",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/index.esm.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "packages/react/src",
    "targets": {
      "build": {
        "executor": "@nx/rollup:rollup",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/react/dist",
          "main": "packages/react/src/index.ts",
          "tsConfig": "packages/react/tsconfig.lib.json",
          "compiler": "swc",
          "format": [
            "esm"
          ]
        }
      }
    }
  },
  "dependencies": {
    "@23blocks/contracts": "*",
    "@23blocks/transport-http": "*",
    "@23blocks/block-authentication": "*",
    "@23blocks/block-search": "*",
    "@23blocks/block-products": "*",
    "@23blocks/block-crm": "*",
    "@23blocks/block-content": "*",
    "@23blocks/block-geolocation": "*",
    "@23blocks/block-conversations": "*",
    "@23blocks/block-files": "*",
    "@23blocks/block-forms": "*",
    "@23blocks/block-assets": "*",
    "@23blocks/block-campaigns": "*",
    "@23blocks/block-company": "*",
    "@23blocks/block-rewards": "*",
    "@23blocks/block-sales": "*",
    "@23blocks/block-wallet": "*",
    "@23blocks/block-jarvis": "*",
    "@23blocks/block-onboarding": "*",
    "@23blocks/block-university": "*",
    "@swc/helpers": "~0.5.11"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  }
}
