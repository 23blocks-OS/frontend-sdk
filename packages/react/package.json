{
  "name": "@23blocks/react",
  "version": "0.0.1",
  "description": "React bindings for 23blocks SDK - hooks and context providers",
  "license": "MIT",
  "author": "23blocks <hello@23blocks.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/23blocks-OS/frontend-sdk.git",
    "directory": "packages/react"
  },
  "homepage": "https://github.com/23blocks-OS/frontend-sdk/tree/main/packages/react",
  "bugs": {
    "url": "https://github.com/23blocks-OS/frontend-sdk/issues"
  },
  "keywords": ["23blocks", "sdk", "react", "hooks", "context", "provider"],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "packages/react/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/react/dist",
          "main": "packages/react/src/index.ts",
          "tsConfig": "packages/react/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      }
    }
  },
  "dependencies": {
    "@23blocks/contracts": "^0.0.1",
    "@23blocks/block-authentication": "^0.0.1",
    "@23blocks/block-search": "^0.0.1",
    "@swc/helpers": "~0.5.11"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  }
}
