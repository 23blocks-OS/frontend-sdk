{
  "name": "@23blocks/contracts",
  "version": "1.0.0",
  "description": "Core type definitions and interfaces for the 23blocks SDK",
  "license": "MIT",
  "author": "23blocks <hello@23blocks.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/23blocks-OS/frontend-sdk.git",
    "directory": "packages/contracts"
  },
  "homepage": "https://github.com/23blocks-OS/frontend-sdk/tree/main/packages/contracts",
  "bugs": {
    "url": "https://github.com/23blocks-OS/frontend-sdk/issues"
  },
  "keywords": [
    "23blocks",
    "sdk",
    "contracts",
    "types",
    "typescript"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "packages/contracts/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/contracts/dist",
          "main": "packages/contracts/src/index.ts",
          "tsConfig": "packages/contracts/tsconfig.lib.json",
          "skipTypeCheck": true,
          "stripLeadingPaths": true
        }
      }
    }
  },
  "dependencies": {
    "@swc/helpers": "~0.5.11"
  }
}
