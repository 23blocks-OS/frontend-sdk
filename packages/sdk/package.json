{
  "name": "@23blocks/sdk",
  "version": "6.1.2",
  "description": "23blocks SDK - unified meta-package re-exporting all blocks and utilities",
  "license": "MIT",
  "author": "23blocks <hello@23blocks.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/23blocks-OS/frontend-sdk.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://23blocks.com",
  "bugs": {
    "url": "https://github.com/23blocks-OS/frontend-sdk/issues"
  },
  "keywords": [
    "23blocks",
    "sdk",
    "typescript-sdk",
    "api-client",
    "backend-as-a-service",
    "baas",
    "firebase-alternative",
    "supabase-alternative",
    "headless-backend",
    "react-backend",
    "angular-backend",
    "nextjs-backend",
    "jsonapi",
    "authentication-sdk",
    "ecommerce-sdk",
    "crm-sdk",
    "saas-boilerplate",
    "full-stack-toolkit",
    "modular-architecture",
    "enterprise-sdk",
    "nodejs",
    "javascript"
  ],
  "type": "module",
  "main": "./dist/index.esm.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "packages/sdk/src",
    "targets": {
      "build": {
        "executor": "@nx/rollup:rollup",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/sdk/dist",
          "main": "packages/sdk/src/index.ts",
          "tsConfig": "packages/sdk/tsconfig.lib.json",
          "compiler": "swc",
          "format": [
            "esm"
          ]
        }
      }
    }
  },
  "dependencies": {
    "@23blocks/contracts": "*",
    "@23blocks/jsonapi-codec": "*",
    "@23blocks/transport-http": "*",
    "@23blocks/block-authentication": "*",
    "@23blocks/block-search": "*",
    "@23blocks/block-products": "*",
    "@23blocks/block-crm": "*",
    "@23blocks/block-content": "*",
    "@23blocks/block-geolocation": "*",
    "@23blocks/block-conversations": "*",
    "@23blocks/block-files": "*",
    "@23blocks/block-forms": "*",
    "@23blocks/block-assets": "*",
    "@23blocks/block-campaigns": "*",
    "@23blocks/block-company": "*",
    "@23blocks/block-rewards": "*",
    "@23blocks/block-sales": "*",
    "@23blocks/block-wallet": "*",
    "@23blocks/block-jarvis": "*",
    "@23blocks/block-onboarding": "*",
    "@23blocks/block-university": "*",
    "@swc/helpers": "~0.5.11"
  }
}
